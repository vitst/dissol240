FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvSolution;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

solvers
{
    p
    {
        solver                  GAMG;
        tolerance               1e-10;
        relTol                  1e-3;
        smoother                GaussSeidel;
        agglomerator            faceAreaPair;
        nCellsInCoarsestLevel   10;
        mergeLevels             1;
        cacheAgglomeration      true;
        minIter                 1;
    }

    U
    {
        type                    coupled;
        solver                  PBiCCCG;
        preconditioner          DILU;
        tolerance               (1e-10 1e-10 1e-10);
        relTol                  (1e-1  1e-1  1e-1);
    }

    C0
    {
        solver                  GAMG;
        tolerance               1e-10;
        relTol                  0;
        smoother                GaussSeidel;
        agglomerator            faceAreaPair;
        nCellsInCoarsestLevel   10;
        mergeLevels             1;
        cacheAgglomeration      true;
        minIter                 1;
    }
}

SIMPLE
{
    nNonOrthogonalCorrectors 0;
    residualControl
    {
        p               1e-8;
        U               1e-8;
    } 
}

relaxationFactors
{
    fields
    {
        p               0.05;
    }
    equations
    {
        U               0.95;
    }
}
          
concentration
{
    nCycle              20;
    tolerance           1e-8;
}

